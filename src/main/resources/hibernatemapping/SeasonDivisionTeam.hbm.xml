<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="true" default-lazy="false">

  <class name="uk.co.mindbadger.footballresultsanalyser.domain.SeasonDivisionTeam" table="season_division_team">

    <composite-id>
      <key-many-to-one name="season" class="uk.co.mindbadger.footballresultsanalyser.domain.SeasonDivision" column="ssn_num"/>
      <key-many-to-one name="division" class="uk.co.mindbadger.footballresultsanalyser.domain.SeasonDivision" column="div_id"/>
      <key-many-to-one name="team" class="uk.co.mindbadger.footballresultsanalyser.domain.Team" column="team_id"/>
    </composite-id>
  </class>
  
</hibernate-mapping>